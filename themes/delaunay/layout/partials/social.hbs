<nav class="social-nav">
	<ul class="social-links">
		{{#each config.contact as |contact social|}}
			<li aria-label="{{contact.title}}">
				{{#if contact.url}}<a href="{{contact.url}}">{{/if}}
					<svg version="1.1" viewBox="0 0 14 16" class="octicon octicon-{{social}}" aria-hidden="true">
						<use xlink:href="#{{social}}" />
					</svg>{{#if contact.text}}{{{contact.text}}}{{/if}}
				{{#if contact.url}}</a>{{/if}}
			</li>
		{{/each}}
	</ul>
</nav>
