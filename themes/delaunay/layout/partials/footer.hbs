<footer>
  <div class="footer-content-wrapper">
    <h3>Get in Touch</h3>
    <ul class="social-links">
      {{#each config.contact as |contact social|}}
        <li aria-label="{{contact.title}}">
          <a href="{{contact.url}}">
            <span class="socicon socicon-{{social}}" title="{{social}}" aria-hidden="true"></span>
          </a>
        </li>
      {{/each}}
    </ul>
  </div>
  <div class="delaunay-canvas-wrapper">
    <canvas id="delaunayFooter" width="500" height="300"></canvas>
  </div>
</footer>
